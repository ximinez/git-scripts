_git_lazyrebase() {
_git_checkout;
};
_git_logdiff() {
_git_log;
};
_git_review() {
_git_checkout;
};
_git_checkoutbranch() {
_git_checkout;
};
_git_squash() {
_git_checkout;
};
_git_rollback() {
_git_add;
};
_git_adddiff() {
_git_add;
};
_git_addiff() {
_git_add;
};
_git_signpush() {
_git_push;
};
